{
  "name": "@worker-tools/router",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js"
  },
  "files": [
    "src",
    "*.js*",
    "*.d.ts*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf *.d.ts index.js router.js url-pattern.js *.js.map *.d.ts.map dist",
    "test": "npm run bundle && cp *.d.ts dist; node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "bundle": "esbuild src/index.ts --bundle --sourcemap --format=esm --target=esnext --outfile=dist/router.js",
    "build": "tsc -d",
    "watch": "tsc -d -w",
    "prepack": "npm run clean && npm run build"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "MIT",
  "dependencies": {
    "@worker-tools/middleware": "workspace:0.0.1",
    "@worker-tools/response-creators": "^1.0.8",
    "urlpattern-polyfill": "^1.0.0-rc5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@worker-tools/location-polyfill": "workspace:^0.3.2-pre.1",
    "esbuild": "^0.14.20",
    "jest": "^27.5.1",
    "jest-environment-miniflare": "^2.3.0",
    "miniflare": "^2.3.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/router.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/router/issues"
  },
  "homepage": "https://github.com/worker-tools/router#readme",
  "keywords": []
}
